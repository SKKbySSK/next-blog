{
  "name": "next-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run update && next build",
    "start": "npm run git:update && next start",
    "update": "npm run git:update",
    "git:update": "npm run git:cd && npm run git:pull",
    "git:cd": "mkdir -p resources && cd resources",
    "git:pull": "if cd resources/blog-resources; then git pull; else git clone https://github.com/SKKbySSK/blog-resources.git resources/blog-resources; fi"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.1.4",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^3.2.1",
    "next": "^11.1.0",
    "postcss-import": "^12.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^4.1.0",
    "react-lottie": "^1.2.3",
    "react-slidez": "0.0.31",
    "react-spring": "^8.0.27",
    "react-syntax-highlighter": "^12.2.1",
    "react-twitter-embed": "^3.0.3",
    "remarkable": "^2.0.1",
    "remarkable-react": "^1.4.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.2",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.35",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-lottie": "^1.2.5",
    "@types/react-syntax-highlighter": "^11.0.4",
    "autoprefixer": "^9.7.6",
    "postcss-cli": "^7.1.1",
    "typescript": "^3.8.3"
  }
}
